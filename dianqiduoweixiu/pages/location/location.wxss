.big_box{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #f7f7f7;
  padding-bottom: 180rpx;
  box-sizing: border-box;
  overflow: scroll;
}
.item{
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.shipping_address_box{
  width: 100%;
  height: 228rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding: 20rpx 0;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1rpx solid #f7f7f7;

}
.userinfo{
  font-weight: 600;
  width: 100%;

}
.phone{
  font-size: 26rpx;
  color: #999;
  margin-bottom:10rpx; 
}
.shipping_address_message{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #999;
}
.message_icon{
  width: 12rpx;
  height: 22rpx;
}

.compile{
  width: 100%;
  height: 114rpx;
  display: flex;
  align-items: center;
  
  justify-content: space-between;
  background: #fff;
  margin-bottom: 16rpx;
}
.default{
  /* width: 170rpx; */
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  color: #129CA3;
}
.wimg{
  width: 34rpx;
  height: 34rpx;
  margin-right:22rpx; 
}
.icon{
  width: 30rpx;
  height: 30rpx
}
.operation{
  /* width: 210rpx; */
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.operation_item{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #129CA3;
  margin-left: 38rpx;
}

.tijiao{
  width:690rpx;
  height:88rpx;
  background:rgba(18,156,163,1);
  border-radius:10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #129CA3;
  color: #fff;
}
.sbmint_box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 180rpx;
  background: #f7f7f7;
  display: flex;
  justify-content: center;
  align-items: center;
}