
.bing_box{
  width: 750rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1rpx solid #ebebeb;
}
.bing{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.time{
  width: 500rpx;
  display: flex;
  justify-content: space-between;
}
.time_start{
  width:230rpx;
  height: 64rpx;
  border: 1rpx solid #D6DBE2;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;

}
.bing_time{
  display: flex;
  align-items: center;
width: 100%;
justify-content: space-between;
}
.bing_money{
  font-weight: 600;
  margin-left: 20rpx;
}
.time_img{
  margin-left: 18rpx;
  width: 13rpx;
  height: 10rpx;

}
.tab_box {
  width: 750rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 16rpx solid #f6f6f6;
}

.tab_tonh {
  position: relative;
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}
.tab_htnd{
  font-weight: 600;
}
.tab_htnd::after{
  content:'';
  position: absolute;
  bottom: 0;
  left: 133rpx;
  width:110rpx;
  height: 4rpx;
  background: #333;
}




.tabList {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1000;
    background: #fff;
    display: -webkit-box;
    -webkit-box-align: center;
    height: 99rpx;
    overflow-x: auto;
    padding-left: 40rpx;
}

.tabList .tabCell {
    margin-right: 60rpx;
    font-size: 28rpx;
    color: #333;
    position: relative;
    line-height: 99rpx;
}

.tabList .tabCell:last-child {
    padding-right: 40rpx;
}

.tabList .active {
    font-weight: bold;
}

.tabList .active::after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #333;
    border-radius: 50rpx;
}

.tabList .active:last-child::after {
    right: 40rpx;
}

.list_cell {
    background: #fff;
    border-bottom: 16rpx solid #f6f6f6;

    padding: 0 40rpx;
}

.list_cell .order_code {
    padding: 30rpx 0 25rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #ebebeb;
}

.list_cell .order_code .code {
    font-size: 28rpx;
    color: #333;
    flex: 1;
    overflow: hidden;
    word-wrap: break-word;
    word-break: break-all;
}

.list_cell .order_code .code .code_number {
    margin-left: 30rpx;
}

.list_cell .order_code .status {
    font-size: 26rpx;
    color: #f36449;
}

.list_cell .goods_box {
    padding: 40rpx 0;
    border-bottom: 1rpx solid #ebebeb;
}

.list_cell .goods_box .goods_msg {
    display: flex;
    justify-content: flex-start;
    padding-bottom: 40rpx;
}

.list_cell .goods_box .goods_msg:last-child {
    padding-bottom: 0;
}

.list_cell .goods_box .goods_msg .goods_thumb {
    width: 200rpx;
    height: 200rpx;
    display: block;
    margin-right: 35rpx;
}

.list_cell .goods_box .goods_msg .goods_data {
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    flex: 1;
    overflow: hidden;
}

.list_cell .goods_box .goods_msg .goods_data .goods_top .goods_title {
    font-size: 30rpx;
    color: #333;
    font-weight: bold;
    display: flex;
}

.list_cell .goods_box .goods_msg .goods_data .goods_top .goods_time {
    font-size: 24rpx;
    color: #999;
    padding-top: 20rpx;
}

.list_cell .goods_data .goods_price {
    font-size: 30rpx;
    color: #333;
    font-weight: bold;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.order_options {
    padding: 32rpx 0;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.order_options .btn {
    margin-left: 53rpx;
    border-radius: 5rpx;
    font-size: 28rpx;
    font-weight: bold;
    text-align: center;
    display: block;
    height: 64rpx;
    line-height: 64rpx;
    width: 190rpx;
    box-sizing: border-box;
    padding: 0;
}

.order_options .white {
    border: 1rpx solid #d6dbe2;
    color: #999;
}

.order_options .black {
    background: #333;
    color: #fff;
}
