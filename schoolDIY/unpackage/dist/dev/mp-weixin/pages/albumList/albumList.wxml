<view class="wrap"><view class="header"><view class="school">{{school}}</view><view class="class">{{grade}}</view><view class="pages color_main">{{"共"+allNum+"页"}}</view></view><view class="list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><navigator class="item" url="{{'../detail/detail?id='+item.id}}" hover-class="none"><image class="cover" src="{{item.img}}" mode="aspectFill" lazy-load="true"></image><view class="name a_row">{{item.name}}</view></navigator></block></block></view><block wx:if="{{list.length==0&&isNoMore}}"><view class="nothing"><image mode="aspectFill" src="/static/nothing.png" lazy-load="true"></image><view>暂无数据~</view></view></block><block wx:else><block wx:if="{{list.length>0&&isNoMore}}"><view class="no_more">暂无更多</view></block><block wx:else><view class="loading"><image src="/static/loading.gif" mode="aspectFill"></image></view></block></block><view class="footer"><block wx:if="{{userInfo.identity==3}}"><navigator class="btn bg_main" open-type="navigateBack" delta="1" hover-class="none">返回首页</navigator></block><block wx:else><view data-event-opts="{{[['tap',[['changeDetail',['$event']]]]]}}" class="btn bg_main" bindtap="__e">修改调整</view></block></view></view>