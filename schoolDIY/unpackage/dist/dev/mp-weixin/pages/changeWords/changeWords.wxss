.wrap {
  background-color: #ffffff;
}
.paper_box {
  margin: 60rpx auto;
  width: 690rpx;
  overflow: hidden;
  position: relative;
  z-index: 0;
}
.paper_box .bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.paper_box .paper_content {
  padding: 50rpx 30rpx;
  color: #813e19;
  position: relative;
  z-index: 0;
}
.paper_box .paper_content .float_item {
  margin-top: 12rpx;
  overflow: hidden;
}
.paper_box .paper_content .label {
  height: 60rpx;
  line-height: 60rpx;
  float: left;
}
.paper_box .paper_content input {
  height: 60rpx;
  border-bottom: 1rpx dashed #efc0a7;
}
.btn_block {
  margin: 100rpx 30rpx;
}

